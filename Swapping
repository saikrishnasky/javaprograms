public class Swapping {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int a = 10, b =20;
		System.out.println("before swapping " + a+" " +b);
		// logic 1
		//int t = a;
		//a=b;
		//b=t;
		//System.out.println("after  swapping "  + a+ "  " +b);*/"
	
		//logic 2
		
		a=a+b;
		b=a-b;
		a=a-b;
		
		System.out.println("after  swapping "  + a+ "  " +b);
				
	}

}
